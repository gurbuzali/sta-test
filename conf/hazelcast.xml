<hazelcast xsi:schemaLocation="http://www.hazelcast.com/schema/config
                               http://www.hazelcast.com/schema/config/hazelcast-config-3.3.xsd"
           xmlns="http://www.hazelcast.com/schema/config"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <group>
        <name>aa-stabilizer-agent</name>
    </group>

    <network>
        <port port-count="200" auto-increment="true">5701</port>
        <join>
            <multicast enabled="false"/>
            <tcp-ip enabled="true">
                <!--MEMBERS-->
            </tcp-ip>
        </join>
    </network>

    <map name="MapStore*">
        <map-store enabled="true">
            <class-name>com.hazelcast.stabilizer.tests.map.helpers.MapStoreWithCounter</class-name>
            <write-delay-seconds>5</write-delay-seconds>
        </map-store>
    </map>

    <map name="MaxSizeMap*">
        <eviction-policy>LRU</eviction-policy>
        <max-size policy="PER_NODE">1000</max-size>
        <eviction-percentage>25</eviction-percentage>
        <min-eviction-check-millis>0</min-eviction-check-millis>
    </map>

    <map name="basicMap*">
        <backup-count>0</backup-count>
        <async-backup-count>0</async-backup-count>
        <statistics-enabled>false</statistics-enabled>
    </map>

</hazelcast>